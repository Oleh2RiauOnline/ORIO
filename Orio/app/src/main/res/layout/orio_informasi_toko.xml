<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B5A9A9">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contextClickable="true"
        android:text="Tambah Data Toko"
        android:textAlignment="center"
        android:textColor="#D83C3C"
        android:textColorHighlight="#F8F8F8"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="41dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="116dp"
        android:layout_height="48dp"
        android:background="#46D512"
        android:text="Tambah"
        android:textColor="#DAF10B"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="400dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="331dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="Nama Toko"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="141dp" />

    <EditText
        android:id="@+id/editTextTextPersonName8"
        android:layout_width="157dp"
        android:layout_height="54dp"
        android:ems="10"
        android:hint="Long"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="324dp" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="331dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Alamat Toko"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="210dp" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="334dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Deskripsi Toko"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="269dp" />

    <EditText
        android:id="@+id/editTextTextPersonName7"
        android:layout_width="149dp"
        android:layout_height="49dp"
        android:ems="10"
        android:hint="Lat"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="330dp" />

</androidx.constraintlayout.widget.ConstraintLayout>