<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <View
        android:id="@+id/view2"
        android:layout_width="409dp"
        android:layout_height="557dp"
        android:background="@drawable/round_border_form"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.379" />

    <View
        android:id="@+id/view3"
        android:layout_width="411dp"
        android:layout_height="125dp"
        android:layout_marginTop="1dp"
        android:background="@drawable/selembayum"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view"
        android:layout_width="413dp"
        android:layout_height="117dp"
        android:background="@drawable/ic_wave_bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contextClickable="true"
        android:text="Tambah Data Toko"
        android:textAlignment="center"
        android:textColor="#D83C3C"
        android:textColorHighlight="#F8F8F8"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="153dp" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="331dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Alamat Toko"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="297dp" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="334dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Deskripsi Toko"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="356dp" />

    <EditText
        android:id="@+id/editTextTextPersonName8"
        android:layout_width="165dp"
        android:layout_height="54dp"
        android:ems="10"
        android:hint="Long"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="411dp" />

    <EditText
        android:id="@+id/editTextTextPersonName7"
        android:layout_width="149dp"
        android:layout_height="49dp"
        android:ems="10"
        android:hint="Lat"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="417dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="331dp"
        android:layout_height="48dp"
        android:drawablePadding="2dp"
        android:drawableTint="#151515"
        android:ems="10"
        android:hint="Nama Toko"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="228dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="116dp"
        android:layout_height="48dp"
        android:background="@drawable/round_border_button"
        android:text="Tambah"
        android:textColor="#DAF10B"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="498dp" />

</androidx.constraintlayout.widget.ConstraintLayout>